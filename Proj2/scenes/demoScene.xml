    <transformations>
        <transformation id="demoTransform">
            <scale x="5" y="1" z="5" />
            <rotate axis="x" angle="-45" />
            <rotate axis="z" angle="-45" />
            <translate x="10" y="0" z="0" />
        </transformation>
        <transformation id="neutralTransformation">
            <translate x="0" y="0" z="0" />
        </transformation>
    </transformations>

    <animations>
        <animation id="experiment">
            <keyframe instant="6">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="10000" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="10">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>

        </animation>
    </animations>

    <primitives>
        <primitive id="soil">
            <rectangle x1="-1" y1="-1" x2="1" y2="1" />
        </primitive>

        <primitive id="walls">
            <cylinder base="4" top="2" height="11" slices="30" stacks="15"/>
        </primitive>

        <primitive id="roof">
            <cylinder base="2.5" top="0" height="2" slices="30" stacks="15"/>
        </primitive>

        <primitive id="door">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>

        <primitive id="window">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
        
        <primitive id="stand">
            <cylinder base="0.05" top="0.05" height="1" slices="30" stacks="15"/>
        </primitive>

        <primitive id="beam">
            <cylinder base="0.1" top="0.1" height="1.4" slices="30" stacks="15"/>
        </primitive>

        <primitive id="joint">
            <sphere radius="0.2" slices="50" stacks="50"/>
        </primitive>

        <primitive id="handrail">
            <torus inner = "0.05" outer = "4.5" slices="100" loops="100" />
        </primitive>

        <primitive id="platform">
            <torus inner = "1.0" outer = "3.5" slices="100" loops="100" />
        </primitive>

        <primitive id="wing_stick">
            <rectangle x1="-0.05" y1="-5.5" x2="0.05" y2="0" />
        </primitive>

        <primitive id="wing_cloth">
            <triangle x1="0" y1="0" z1="0" x2="-3" y2="-5.5" z2="0" x3="0" y3="-5.5" z3="0.5" />
        </primitive>

        <primitive id="auxSphere">
            <sphere radius="10" slices="50" stacks="50" />
        </primitive>

        <primitive id="coneFan">
            <cylinder base="0.4" top="0.05" height="20" slices="30" stacks="15" />
        </primitive>

        <primitive id="bodyFan">
            <cylinder base="0.4" top="0.2" height="20" slices="30" stacks="15" />
        </primitive>

        <primitive id="coneRiver">
            <cylinder base="2" top="2" height="20" slices="30" stacks="15" />
        </primitive>

        <primitive id="arco">
            <torus inner = "2" outer = "13" slices="100" loops="100" />
        </primitive>

        <primitive id="plano">
            <plane npartsU = "5" npartsV = "8" />
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
                <translate x="0" y="0" z="0" /> 
            </transformation>
            <materials>
                <material id="nullMaterial" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="Plane" />
                <!--<componentref id="Soil" /> -->
                <componentref id="ArcoIris" />
                <!--<componentref id="River" />
                <componentref id="Lake" />
                <componentref id="Motor" />
                <componentref id="windFan" />
                <componentref id="Windmill" />
                <componentref id="Sun" />
                <componentref id="Hills" />-->
