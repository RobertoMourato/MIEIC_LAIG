<lxs>
    <scene root="root" axis_length="100"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="200" angle="45">
            <from x="-25" y="5" z="50" />
            <to x="0" y="5" z="0" />
        </perspective>

        <perspective id="viewFromBehind" near="0.1" far="200" angle="45">
            <from x="-35" y="25" z="-48" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
    
        <spot id="spotSun" enabled="0" angle="30" exponent="2">     
            <location x="20" y="45" z="-45" w="0.0" />
            <target x="-15" y="6" z="-5" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1.0" linear="0.0" quadratic="0.00" />
        </spot>
    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/vidral.jpg" />
        <texture id="sunText" file="scenes\images/sun.jpg" />
        <texture id="hillsText" file="scenes\images/forest.jpg" />
        <texture id="water" file="scenes\images/water.jpg" />
        <texture id="metal" file="scenes\images/metal.jpg" />
        <texture id="arcoiris" file="scenes\images/arcoiris.jpg" />
        <texture id="brick_wall" file="scenes/images/brick-wall.jpg" />
        <texture id="cloth" file="scenes/images/cloth.jpg" />
        <texture id="white_wood" file="scenes/images/white-wooden-surface.jpg" />
        <texture id="wooden_door" file="scenes/images/door.jpg" /> 
        <texture id="dark_wood" file="scenes/images/dark-wood.jpg" />
    </textures>

