<lxs>
    <scene root="root" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="50" angle="45">
            <from x="20" y="12" z="20" />
            <to x="0" y="5" z="0" />
        </perspective>

        <perspective id="viewFromBehind" near="0.1" far="50" angle="45">
            <from x="-20" y="12" z="-20" />
            <to x="0" y="-2" z="0" />
        </perspective>
